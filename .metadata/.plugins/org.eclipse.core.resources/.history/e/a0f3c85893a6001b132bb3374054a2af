package gun_2_odev_1;

public class InstructorManager {
	
	public void addInstructor(Instructor instructor) {
		System.out.println(instructor.name + ", eðitmen olarak eklendi.");
	}
	
	public void deleteInstructor(Instructor instructor) {
		System.out.println(instructor.name + ", eðitmen listesinden silindi.");
	}
	
	public void updateInstructor(Instructor instructor) {
		System.out.println(instructor.name + " eðitmenine ait bilgiler güncellendi.")
	}
	
	public void selectInstructor(Instructor instructor) {
		System.out.println(instructor.name + " seçildi.");
	}

}

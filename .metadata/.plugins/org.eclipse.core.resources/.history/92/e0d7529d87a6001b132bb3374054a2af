package gun_2_canli_tekrar;

public class Product {
	
	public Product() {		//Bunu yapmasaydýk sadece aþaðýdaki constructor tipine müsaade edecekti.
		
	}
	
	public Product(int id, String name, int unitPrice, String details) {
		this.id=id;
		this.name=name;
		this.unitPrice=unitPrice;
		this.details=details;
	}
		
	int id;
	String name;
	int unitPrice;
	String details;

}

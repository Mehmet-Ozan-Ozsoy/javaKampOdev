package gun_2_odev_1;

public class Instructor {
	
	public Instructor() {
		
	}
	
	public Instructor(int id, int TCkimlikNo, String firstName, String lastName) {
		this();
		this.id=id;
		this.firstName=firstName;
		this.lastName=lastName;
	}
	
	int id;
	int TCkimlikNo;
	String firstName;
	String lastName;

}

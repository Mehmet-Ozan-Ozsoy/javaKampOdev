package gun_2_odev_2.classesWithAttributes;

public class Product {

	
	//Attribute / Field bloku
	int id;						//Java'da bunlar default olarak "public"tir (YANÝ BU ATTRIBUTE ASLINDA: public int id;). C#'ta ise default hali private olduðu için biz bunu açýk þekilde public int id; gibi bir gösterimle yazýyorduk.
	String name;
	String description;
	double price;
	int stockAmount;
	
}

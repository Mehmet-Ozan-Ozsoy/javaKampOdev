package gun_2_odev_2.classes;

public class Main {

	public static void main(String[] args) {
		
		CustomerManager customerManager = new CustomerManager();	//customerManager class'ından değişkenin stack'tedir. Ona atayacağın değer ise heap'te olacaktır. Bunun için "new" bir class değişkeni tanımlamalıyız.  
		CustomerManager customerManager2 = new CustomerManager();
		customerManager=customerManager2;							//Ama buradaki gibi, ilk değişkenin değerine ikincininkini atarsan, en azından ilkine ait "new ClassManager()"
		customerManager.Add();
		customerManager.Remove();
		customerManager.Update();
	}

}
